    "@things-factory/scene-chartjs": "^3.0.0-beta.80",
    "@things-factory/scene-clock": "^3.0.0-beta.80",
    "@things-factory/scene-clone": "^3.0.0-beta.80",
    "@things-factory/scene-data-transform": "^3.0.0-beta.80",
    "@things-factory/scene-excel": "^3.0.0-beta.80",
    "@things-factory/scene-firebase": "^3.0.0-beta.80",
    "@things-factory/scene-forklift": "^3.0.0-beta.80",
